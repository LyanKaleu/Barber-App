{
  "expo": {
    "name": "Barber-App",
    "slug": "Barber-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-barber.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/SPLASH.png",
      "resizeMode": "contain",
      "backgroundColor": "#312E38"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-barber.png",
        "backgroundColor": "#312E38"
      },
      "package": "com.barberapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "scheme": "barberApp",
    "platforms": [
      "ios",
      "android"
    ],
    "deepLinking": true
  }
}
